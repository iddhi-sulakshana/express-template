{
  "name": "express-template",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "build:tsc": "tsc --build",
    "build:alias": "tsc-alias",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "dev:bun": "bun run --watch src/index.ts",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,css,md,json,scss,html}' --config ./.prettierrc",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push"
  },
  "optionalDependencies": {
    "@types/bun": "latest"
  },
  "dependencies": {
    "argon2": "^0.44.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv-safe": "^9.1.0",
    "drizzle-orm": "^0.44.4",
    "express": "^5.1.0",
    "husky": "^9.1.7",
    "ioredis": "^5.7.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nanoid": "^5.1.5",
    "postgres": "^3.4.7",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/dotenv-safe": "^8.1.6",
    "@types/express": "^5.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.17.2",
    "@types/pg": "^8.15.5",
    "@types/swagger-ui-express": "^4.1.8",
    "drizzle-kit": "^0.31.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.0.0"
  }
}
